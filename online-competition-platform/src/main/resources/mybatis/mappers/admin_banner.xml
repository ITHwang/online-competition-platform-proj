<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	<mapper namespace = "mappers.admin_banner"> 
	<!-- 리절트맵 정의 -->
	<resultMap id = "BannerVO" type = "adminBannerVO"> <!-- CompetFile의 resultMap -->
        <result property = "banner_id" column = "banner_id" />
		<result property = "banner_File_Name" column = "banner_file_name" />
		<result property = "banner_File_Type" column = "banner_file_type" />
	</resultMap>
<!-- 카테고리 데이터 가져오기 -->
	<select id="selectAllBannerList" resultMap="BannerVO">
  	<![CDATA[
				select banner_id,banner_file_name,banner_file_type from banner_file
   	]]>
	</select>

	<select id="selectCategName" resultMap="CategResult">
  	<![CDATA[
				select categ_name
				from category
   	]]>
	</select>


	
</mapper>